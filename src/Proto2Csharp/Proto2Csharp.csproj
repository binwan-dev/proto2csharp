<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <PackAsTool>true</PackAsTool>
    <ToolCommandName>proto2csharp</ToolCommandName>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <Version>0.0.4</Version>
    <PackageVersion>0.0.4</PackageVersion>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageId>proto2csharp</PackageId>
    <Authors>binwan</Authors>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="CommandLineParser" Version="2.8.0" />
    <PackageReference Include="Fnlinker.Generic.Hosting" Version="1.2.9" />
    <PackageReference Include="Kadder" Version="0.1.0-dev.20.1.3" />
  </ItemGroup>

</Project>
